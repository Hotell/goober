{
  "name": "@goober/styled-props",
  "version": "0.1.0",
  "description": "Addon for goober that adds styled.properties",
  "main": "dist/goober.js",
  "module": "dist/goober.mjs",
  "umd:main": "dist/goober.umd.js",
  "source": "src/index.js",
  "files": [
    "src",
    "README.md",
    "dist",
    "package.json",
    "typings.json",
    "goober.d.ts"
  ],
  "scripts": {
    "test": "npm run test-unit -- --ci --coverage && npm run build",
    "test-unit": "jest",
    "clean": "rm -rf dist",
    "build": "npm run clean && microbundle --entry src/index.js --output dist --name goober --no-sourcemap",
    "debug": "microbundle --entry src/index.js --output debug --name goober --no-sourcemap --no-compress",
    "dev": "npm run clean && microbundle watch --entry src/index.js --output dist --name goober",
    "deploy": "npm run build && npm publish"
  },
  "keywords": [
    "css-in-js",
    "goober",
    "styled",
    "emotion",
    "styled-components",
    "javascript",
    "react",
    "preact"
  ],
  "author": "Cristian <botecristian@yahoo.com>",
  "repository": "https://github.com/cristianbote/goober",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "csstype": "^2.6.3",
    "jest": "^24.1.0",
    "microbundle": "^0.9.0",
    "goober": "../../"
  }
}
